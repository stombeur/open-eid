{
    "name": "Open-eID",
    "version": "0.1.7",
    "description": "Open-eID extension (read & sign)",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },  
    "browser_action": {
    },         
    "optional_permissions": [
      "tabs",
      "<all_urls>",
      "webNavigation"
    ],
    "permissions": [
      "nativeMessaging",
      "activeTab"
    ],    
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },    
    "manifest_version": 2
  }