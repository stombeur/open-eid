{
    "name": "Open-eID",
    "version": "0.1.8",
    "description": "Open-eID extension (read & sign)",
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": {
        "16": "icon16.png",
        "32": "icon32.png"
      }     
    },
    "optional_permissions": [
      "tabs",
      "<all_urls>",
      "webNavigation"
    ],            
    "permissions": [
      "nativeMessaging",
      "activeTab"
    ],    
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },    
    "applications": {
      "gecko": {
        "id": "firefox@open-eid.eu.org",
        "strict_min_version": "50.0"
      }
    },
    "manifest_version": 2
  }